- cache do
  .row
    .col-md-offset-3.col-md-6.nickbox data-toggle="tooltip" data-placement="top" title="#{t('.click_to_copy')}"
      .tooltip-to-copy
        <div data-pjax-container data-clipboard-target='yournameishere' id="yournameishere">
          = t('.name')
        </div>
        = t('.get_nick')
      #sexybuttons
        = link_to t('.generate_name'), '#',
          name: 'male',
          id: 'get_nick'
